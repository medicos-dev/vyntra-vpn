{
  "version": 2,
  "builds": [
    {
      "src": "api/vpngate.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vpngate",
      "dest": "/api/vpngate.js"
    }
  ],
  "functions": {
    "api/vpngate.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}